Start Program

Write "Testing Symbolic Operators"
Write ""

Set a to 10
Set b to 5

Write "Variables: a = 10, b = 5"
Write ""

Write "=== Arithmetic ==="
Set sum to a + b
Write "a + b = "
Write sum

Set diff to a - b
Write "a - b = "
Write diff

Set prod to a * b
Write "a * b = "
Write prod

Set quot to a / b
Write "a / b = "
Write quot

Write ""
Write "=== Comparisons ==="

If a > b
    Write "a > b: TRUE"
End If

If b < a
    Write "b < a: TRUE"
End If

If a == 10
    Write "a == 10: TRUE"
End If

If a >= 10
    Write "a >= 10: TRUE"
End If

If b <= 5
    Write "b <= 5: TRUE"
End If

If a != b
    Write "a != b: TRUE"
End If

Write ""
Write "Symbolic operators working!"

End Program
